<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meis.basemodule">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ActivityModuleActivity"></activity>
        <activity android:name=".fragment.FragmentModuleActivity"></activity>
        <activity android:name=".dialog.DialogModuleActivity"></activity>
        <activity android:name=".activity.StatusActivity"></activity>
        <activity android:name=".activity.CusStatusActivity"></activity>
        <activity android:name=".activity.RefreshActivity"></activity>
        <activity android:name=".activity.SimpleListActivity"></activity>
        <activity android:name=".activity.MulListActivity"></activity>
    </application>

</manifest>